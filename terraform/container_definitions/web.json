[
  {
    "name": "prum_portfolio_web",
    "image": "299271904246.dkr.ecr.ap-northeast-1.amazonaws.com/prum-portfolio-web:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/web",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "ecs-web"
      }
    },
    "command": [
      "rake", "cmd:setup"
    ],
    "environment": [
      {
        "name": "RAILS_ENV",
        "value": "production"
      }
    ],
    "secrets": [
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "/web/rails-master-key"
      }
    ]
  }
]